            <property name="dataSource">Data Source</property>
            <method name="open"><![CDATA[                importPackage(Packages.java.util);

                var dataList = reportContext.getAppContext().get("DATA_LIST");

                if (dataList == null) {
                    dataList = new ArrayList();
                }

                iterator = dataList.iterator();

                // Debug
                try {
                    Packages.java.lang.System.out.println("BIRT: DATA_LIST size = " + dataList.size());
                } catch(e) {}]]></method>
            <method name="fetch"><![CDATA[                if (iterator != null &amp;&amp; iterator.hasNext()) {

                    var rowData = iterator.next();

                    var col1Field = params["column1_field"];
                    var col2Field = params["column2_field"];
                    var col3Field = params["column3_field"];

                    try {
                        Packages.java.lang.System.out.println(
                           "BIRT params: " + col1Field + ", " + col2Field + ", " + col3Field
                        );
                    } catch(e) {}

                    var col1Name = params["column1_name"];
                    var col2Name = params["column2_name"];
                    var col3Name = params["column3_name"];

                    try {
                        Packages.java.lang.System.out.println(
                           "BIRT names: " + col1Name + ", " + col2Name + ", " + col3Name
                        );
                    } catch(e) {}

                    function getValue(obj, key) {
                        if (obj == null || key == null) return "";

                        try {
                            if (typeof obj.get == "function") {
                                var v = obj.get(key);
                                if (v != null) return String(v);
                            }
                        } catch(e) {}

                        try {
                            var v2 = obj[key];
                            if (v2 != null) return String(v2);
                        } catch(e) {}

                        try {
                            var getterName = "get" + key.substring(0,1).toUpperCase() + key.substring(1);
                            if (typeof obj[getterName] == "function") {
                                var v3 = obj[getterName]();
                                if (v3 != null) return String(v3);
                            }
                        } catch(e) {}

                        return "";
                    }

                    row["column1"] = getValue(rowData, col1Field);
                    row["column2"] = getValue(rowData, col2Field);
                    row["column3"] = getValue(rowData, col3Field);

                    try {
                        Packages.java.lang.System.out.println(
                           "ROW: " + row["column1"] + " | " + row["column2"] + " | " + row["column3"]
                        );
                    } catch(e) {}

                    return true;
                }

                return false;]]></method>
